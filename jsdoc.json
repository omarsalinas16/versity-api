{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"excludePattern": "(node_modules|docs|public|bin)",
		"include": ["README.md"]
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}